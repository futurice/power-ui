{
  "name": "power-ui",
  "private": true,
  "version": "1.0.0",
  "description": "power.futurice.com new UI",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src/**/*.js; exit 0",
    "watch:lint": "esw -w src",
    "watch:static": "cpx \"src/static/**/*\" dist --watch",
    "make-power-ui-symlink": "cd node_modules && ln -nsf ../src power-ui",
    "prewatch": "npm run make-power-ui-symlink",
    "prebuild": "npm run make-power-ui-symlink",
    "build": "BUILD_DEV=false webpack",
    "watch": "npm run watch:static & npm run watch:lint & webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/futurice/power-ui.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/futurice/power-ui/issues"
  },
  "homepage": "https://github.com/futurice/power-ui#readme",
  "dependencies": {
    "@cycle/core": "3.1.x",
    "@cycle/dom": "5.1.x",
    "@cycle/http": "3.1.x",
    "lodash": "^3.10.1",
    "moment": "^2.10.6"
  },
  "devDependencies": {
    "uglify-js": "^2.4.24",
    "autoprefixer-core": "^5.1.11",
    "babel-core": "^5.2.17",
    "babel-eslint": "^4.0.5",
    "babel-loader": "^5.3.1",
    "cpx": "^1.2.1",
    "css-loader": "^0.15.1",
    "eslint": "^1.1.0",
    "eslint-watch": "^1.2.5",
    "extract-text-webpack-plugin": "^0.8.1",
    "node-sass": "^3.3.2",
    "postcss-loader": "^0.4.3",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  }
}
